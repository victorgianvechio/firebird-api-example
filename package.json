{
  "name": "firebird-api",
  "version": "1.0.0",
  "description": "Firebird API",
  "main": "build/server.js",
  "keywords": [
    "api",
    "firebird"
  ],
  "author": "Victor Gianvechio <victor.gianvecchio@live.com>",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "dev": "nodemon src/server.js",
    "dev:browser": "start http://127.0.0.1:8080/api/v1 & nodemon src/server.js ",
    "dev:debug": "nodemon --inspect src/loader.js",
    "eslint:fix": "eslint --fix src --ext .js",
    "env": "sucrase-node src/utils/dotenvGenerator.js",
    "token": "sucrase-node src/utils/tokenGenerator.js",
    "build": "babel src -d build --copy-files",
    "start": "node build/server.js",
    "test:db": "sucrase-node src/test/firebirdConnection.js"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.13.4"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "node-firebird": "^0.9.9",
    "randomstring": "^1.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sucrase": "^3.12.1"
  }
}
